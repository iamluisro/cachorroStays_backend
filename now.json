{
  "name": "cachorroStays",
  "version": 2,
  "public": true,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@cs-db-user",
    "DB_PASSWORD": "@cs-db-ps",
    "DB_HOST": "@cs-db-host",
    "DB_NAME": "@cs-db-name"
  }
}

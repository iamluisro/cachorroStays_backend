{
  "name": "cachorroStays",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
  "env": {
    "DB_USER": "@cachorrostays-db-user-readwrite",
    "DB_PASSWORD": "@cachorrostays-db-ps-readwrite",
    "DB_HOST": "@cachorrostays-db-host-readwrite",
    "DB_NAME": "@cachorrostays-db-name"
  }
}
